
==aws s3 command line

--list bucket
./aws s3api list-buckets 


--create bucket
./aws s3api create-bucket \
    --bucket test \
    --region us-west-2 \
    --create-bucket-configuration LocationConstraint=us-west-2


--put object
./aws s3api put-object --bucket text-content --key 1 --body aws_readme


--list object
./aws s3api list-objects --bucket test


--download object
./aws s3api get-object --bucket test --key 1  readmefroms31


--delete object
./aws s3api delete-object --bucket test --key 1